<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestSuite">
    <!--    thread-count="2" parallel="tests"-->
    <!--    Cross Browser Testing-->
    <!--    Parameter to pass the value to WebAPI-->
    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <!--<parameter name="cloudEnvName" value="saucelab"/>-->
    <parameter name="os" value="windows"/>
<!--     <parameter name="os" value="OS X"/>-->
    <parameter name="os_version" value="7"/>
<!--        <parameter name="os_version" value="catalina"/>-->
<!--        <parameter name="browserName" value="chrome"/>-->
<!--        <parameter name="browserName" value="firefox"/>-->
<!--        <parameter name="browserVersion" value="79"/>-->
    <parameter name="url" value="https://www.geico.com"/>

    <test name="Geico Home">
        <parameter name="browserName" value="chrome"/>
        <parameter name="browserVersion" value="85"/>
        <classes>
            <class name="geicoHomeTest.GeicoHomeTest">
                <methods>
                    <exclude name="testSearchBoxCheck"></exclude>

                </methods>
            </class>

            <!--            <class name="testhome.RegistrationPageTest"></class>-->

        </classes>

    </test>


</suite>





